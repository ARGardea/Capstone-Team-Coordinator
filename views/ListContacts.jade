extends BaseLayout

block content
    div(class='main')
        -if(typeof(list) !== 'undefined') {
            each item in list
                |#{item.username} ~ ~
                a(href='/Profile?username=#{item.username}', class='contactLink') View Profile 
                | ~ ~
                a(href='/SendNote?targetUsername=#{item.username}', class='contactLink') Send Note
                | ~ ~
                a(href='/RemoveContact?targetID=#{item._id}', class='contactLink') Remove Contact
                br
        -}